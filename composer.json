{
    "name": "chromabits/laravel-tutum",
    "description": "Tutum API service for Laravel",
    "require": {
        "laravel/framework": "5.0.x-dev",
        "chromabits/tutum-php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "0.9.*"
    },
    "authors": [
        {
            "name": "Eduardo Trujillo",
            "email": "ed@chromabits.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chromabits\\TutumClient\\": "src/Chromabits/TutumClient/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chromabits\\Tests\\": "tests/Chromabits/"
        }
    },
    "minimum-stability": "dev"
}
