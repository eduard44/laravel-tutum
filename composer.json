{
    "name": "chromabits/laravel-tutum",
    "description": "Tutum API service for Laravel",
    "license": "MIT",
    "require": {
        "laravel/framework": "~5.0",
        "chromabits/tutum-php": "dev-master",
        "predis/predis": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "0.9.*"
    },
    "authors": [
        {
            "name": "Eduardo Trujillo",
            "email": "ed@chromabits.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chromabits\\TutumClient\\": "src/Chromabits/TutumClient/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chromabits\\Support\\": "tests/Chromabits/Support",
            "Tests\\Chromabits\\TutumClient\\": "tests/Chromabits/TutumClient"
        }
    },
    "minimum-stability": "stable"
}
